{
  "name": "alinode-faas-insight",
  "version": "0.0.1",
  "description": "Alinode Faas Insight 观测性平台，可以对您的应用进行指标（qps、rt、errorRate）、链路、异常的监控。",
  "private": false,
  "main": "dist/index",
  "dependencies": {
    "@alicloud/fc2": "^2.2.2",
    "@midwayjs/decorator": "^2.3.0",
    "@midwayjs/egg-layer": "^2.8.11",
    "@midwayjs/serverless-fc-starter": "^2.8.11",
    "@midwayjs/simple-lock": "^1.1.4",
    "@midwayjs/web": "^2.8.11",
    "@pandorajs/semantic-conventions": "^3.0.2",
    "ali-oss": "^6.15.2",
    "aliyun-sdk": "^1.12.3",
    "egg": "^2.0.0",
    "egg-cors": "^2.2.3",
    "egg-passport": "^2.1.1",
    "egg-view-xtpl": "^1.2.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/egg-ts-helper": "^1.0.1",
    "@midwayjs/mock": "^2.3.0",
    "@types/jest": "^26.0.10",
    "@types/node": "14",
    "cross-env": "^6.0.0",
    "egg-scripts": "^2.10.0",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^3.9.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "pre:publish": "rm -rf ./dist && npm run build:ts && cp ./package.json ./dist/",
    "build:ts": "tsc && npm run copy:static && npm run copy:template",
    "copy:static": "cp -r ./src/app/public ./dist/dist/app/ && cp -r ./src/app/view ./dist/dist/app/",
    "copy:template": "cp ./src/index.origin.js ./dist/index.js && cp ./src/template.yaml ./dist/",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts --framework @midwayjs/web",
    "test": "midway-bin test",
    "cov": "midway-bin cov",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "npm run pre:publish"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-integration": {
    "lifecycle": {
      "after:package:installDevDep": "npm run build"
    }
  },
  "author": "jiakun.dujk",
  "license": "MIT"
}
